{
  "name": "@cli-trace/renderer-web",
  "version": "1.0.0",
  "description": "Web renderer for CLI-Trace: SVG and Canvas animation rendering",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "svg",
    "canvas",
    "animation",
    "stroke",
    "web",
    "renderer"
  ],
  "author": "Grisha",
  "license": "MIT",
  "dependencies": {
    "@cli-trace/core": "workspace:*",
    "@cli-trace/parser-svg": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0"
  }
}
